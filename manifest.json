{
	"name": "Language Keyboard",
	"version": "0.1.0.0",
	"manifest_version": 2,
	"description": "Lets you type Lushootseed into OneNote on the web",
	"icons": {
		"16": "icons/Keyboard16x16.png",
		"32": "icons/Keyboard32x32.png",
		"48": "icons/Keyboard48x48.png",
		"128": "icons/Keyboard128x128.png"
	},
	"permissions": [
		"storage"
	],
	"browser_action": {
		"default_popup": "popup/popup.html"
	}
}